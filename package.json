{
  "name": "puppeteer",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "empty_app": "taskset -c 1 npx tsc -p empty_app/tsconfig.json && node dist/empty_app/empty_app/index.js",
    "page": "taskset -c 1 npx tsc -p verify_page/tsconfig.json && node dist/verify_page/verify_page/index.js",
    "www_server_build": "taskset -c 1 npx tsc -p www_server/tsconfig.json",
    "www_server": "node dist/www_server/www_server/index.js",
    "futsal_build": "taskset -c 1 npx tsc -p src/tsconfig.json",
    "futsal_1vs1": "HX_ROOM_CONFIG_SELECTOR=1vs1 HX_ROOM_CONFIG_SUBSELECTOR=1 node dist/src/src/index.js",
    "futsal_3vs3_1": "HX_ROOM_CONFIG_SELECTOR=3vs3 HX_ROOM_CONFIG_SUBSELECTOR=1 node dist/src/src/index.js",
    "futsal_3vs3_2": "HX_ROOM_CONFIG_SELECTOR=3vs3 HX_ROOM_CONFIG_SUBSELECTOR=2 node dist/src/src/index.js",
    "futsal_3vs3_3": "HX_ROOM_CONFIG_SELECTOR=3vs3 HX_ROOM_CONFIG_SUBSELECTOR=3 node dist/src/src/index.js",
    "discord_build": "taskset -c 1 npx tsc -p discord_client/tsconfig.json",
    "discord_1vs1": "HX_SELECTOR=1vs1 HX_SUBSELECTOR=1 node dist/discord_client/discord_client/index.js",
    "discord_3vs3": "HX_SELECTOR=3vs3 HX_SUBSELECTOR=1 node dist/discord_client/discord_client/index.js",
    "market_server_build": "taskset -c 1 npx tsc -p market_server/tsconfig.json",
    "market_server": "HX_SELECTOR=3vs3 node dist/market_server/market_server/index.js",
    "monitoring": "taskset -c 1 npx tsc -p monitoring/tsconfig.json && node dist/monitoring/monitoring/index.js",
    "accu_stats_build": "taskset -c 1 npx tsc -p accu_stats/tsconfig.json",
    "accu_stats_3vs3": "HX_SELECTOR=3vs3 node dist/accu_stats/accu_stats/index.js",
    "accu_stats_1vs1": "HX_SELECTOR=1vs1 node dist/accu_stats/accu_stats/index.js",
    "start": "npm run futsal_3vs3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^5.0.0",
    "async-mutex": "^0.5.0",
    "bcryptjs": "^3.0.2",
    "canvas": "^3.1.0",
    "discord.js": "^14.18.0",
    "express": "^4.21.2",
    "fs-extra": "^11.3.0",
    "glicko2": "^1.2.1",
    "global-agent": "^3.0.0",
    "haxball.js": "^2.3.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "msgpackr": "^1.11.2",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.3.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sqlite3": "^5.1.7",
    "stripe": "^17.7.0",
    "ts-node": "^10.9.2",
    "uninstall": "^0.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "typescript": "^5.7.3"
  }
}
